    "from PIL import Image\n",
    "\n",
    "# 打开图像\n",
    "img = Image.open(\"data/md.png\")\n",
    "\n",
    "# 调整大小为 64x64\n",
    "img_resized = img.resize((32, 32))\n",
    "\n",
    "# 保存处理后的图像\n",
    "img_resized.save(\"data/md32.png\")\n",
    "\n",
    "print(\"png saved\")\n"
   ],
